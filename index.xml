<?xml version="1.0" ?>
<book>
  <section id="seed">
    <pathname name="themeDir">
      <description>
	themeDir is a directory containing templates and css for the site
      </description>
    </pathname>
    <xref linkend="git-core">
      <bin name="git" />
    </xref>
    <build>
      <xref linkend="build-essential">
	<bin name="g++" />
      </xref>
      <xref linkend="git-core">
	<bin name="git" />
	<url>seed</url>
      </xref>      
      <xref linkend="drop">
	<bin name="dws" />
	<etc name="./prefix.mk" />
	<etc name="./suffix.mk" />
	<etc name="./configure.sh" />
      </xref>
      <xref linkend="contrib/boost">
	<include name="./boost/version.hpp" />
	<lib name="boost_date_time" />
	<lib name="boost_filesystem" />
	<lib name="boost_program_options" />
	<lib name="boost_regex" />
	<lib name="boost_system" />
      </xref>
    </build>
  </section>
</book>
